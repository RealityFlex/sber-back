{ 
    "endpoint": "/get_rows",
    "method": "GET",
    "output_encoding": "no-op",
    "input_headers": ["*"],
    "input_query_strings": ["*"],
    "extra_config": {
        "auth/validator": {
            "alg": "RS256",
            "jwk_url": "{{ $host }}/jwk",
            "disable_jwk_security": true,
            "operation_debug": true,
            "propagate_claims": [
            ["exp", "exp"],
            ["sub", "sub"]
            ]
        }
    },
    "backend": [
        {
