{{define "DataGrip"}}

{{$host := .}}

{ 
    "endpoint": "/docs",
    "method": "GET",
    "output_encoding": "no-op",
    "input_headers": ["*"],
    "input_query_strings": ["*"],
    "backend": [
        {
        "host": ["{{ $host }}"],
        "url_pattern": "/docs",
        "method": "GET"
        }
    ]
},
